<% if current_user.project_likes.find_by(project_id: @project.id) %>
<%= link_to 'いいね！済み', hacker_project_unlike_path(@project.id), method: 'delete', remote: true, class: "btn btn-primary" %>
<% else %>
<%= link_to 'いいね！', hacker_project_like_path(@project.id), method: 'post', remote: true, class: "btn btn-default" %>
<% end %>